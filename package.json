{
  "name": "everest-forms",
  "title": "Everest Forms",
  "version": "3.0.2",
  "homepage": "https://github.com/wpeverest/everest-forms/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wpeverest/everest-forms.git"
  },
  "license": "GPL-3.0+",
  "main": "Gruntfile.js",
  "scripts": {
    "dev": "cross-env BABEL_ENV=default webpack --watch",
    "prepare": "husky install",
    "build": "cross-env ./bin/build-zip.sh",
    "build:core": "grunt && npm run makepot && cross-env BABEL_ENV=default NODE_ENV=production webpack",
    "build:zip": "npm run build && composer install && npm run build:core",
    "watch": "cross-env BABEL_ENV=default webpack --watch",
    "lint:js": "eslint assets/js --ext=js",
    "makepot": "composer run-script makepot",
    "git:update-hooks": "node ./node_modules/husky/husky.js install",
    "prephp-install": "wp-env start",
    "prelint-php": "wp-env run composer 'install --no-interaction'",
    "lint-php": "wp-env composer run-script phpcs",
    "test-php": "npm run lint-php && npm run test-unit-php",
    "test-php:watch": "wp-env run composer run-script test:watch",
    "pretest-unit-php": "wp-env start",
    "test-unit-php": "wp-env run phpunit 'phpunit --configuration=/var/www/html/wp-content/plugins/everest-forms/phpunit.xml.dist --verbose'"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/eslint-parser": "7.25.1",
    "@babel/preset-typescript": "7.23.3",
    "@types/wordpress__block-editor": "^11.5.11",
    "@wordpress/babel-preset-default": "6.5.1",
    "@wordpress/block-editor": "^12.12.0",
    "@wordpress/blocks": "^12.29.0",
    "@wordpress/browserslist-config": "4.1.1",
    "@wordpress/components": "^25.16.0",
    "@wordpress/scripts": "^27.3.0",
    "@wordpress/server-side-render": "^4.30.0",
    "autoprefixer": "10.4.20",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.44.0",
    "grunt": "1.5.3",
    "grunt-contrib-compress": "2.0.0",
    "grunt-contrib-concat": "2.1.0",
    "grunt-contrib-cssmin": "4.0.0",
    "grunt-contrib-jshint": "3.2.0",
    "grunt-contrib-uglify": "5.2.2",
    "grunt-contrib-watch": "1.1.0",
    "grunt-phpcs": "0.4.0",
    "grunt-postcss": "0.9.0",
    "grunt-rtlcss": "2.0.2",
    "grunt-sass": "3.1.0",
    "grunt-stylelint": "0.18.0",
    "husky": "7.0.4",
    "lint-staged": "12.5.0",
    "node-sass": "7.0.3",
    "sass": "1.78.0",
    "stylelint": "14.11.0",
    "stylelint-config-wordpress": "17.0.0",
    "webpack": "5.93.0",
    "webpack-cli": "4.10.0"
  },
  "engines": {
    "node": ">=8.9.3",
    "npm": ">=5.5.1"
  },
  "lint-staged": {
    "*.php": [
      "php -d display_errors=1 -l",
      "composer run-script phpcs-pre-commit"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "git add"
    ],
    "!(*.min).js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@tanstack/react-query": "4.36.1",
    "@tanstack/react-query-devtools": "5.53.2",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-beautiful-dnd": "13.1.7",
    "@types/react-color": "3.0.10",
    "@types/react-datepicker": "4.19.4",
    "@types/react-dom": "18.2.18",
    "@types/react-select": "5.0.1",
    "@wordpress/api-fetch": "6.51.0",
    "@wordpress/data": "^9.22.0",
    "@wordpress/i18n": "4.54.0",
    "file-loader": "6.2.0",
    "framer-motion": "11.0.20",
    "grunt-cli": "1.4.3",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-grid-system": "8.2.0",
    "react-icons": "5.2.1",
    "react-player": "2.16.0",
    "react-router-dom": "6.22.3"
  }
}
