{
  "name": "everest-forms",
  "title": "Everest Forms",
  "version": "1.4.9",
  "homepage": "https://wpeverest.com/wordpress-plugins/everest-forms/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wpeverest/everest-forms.git"
  },
  "license": "GPL-3.0+",
  "main": "Gruntfile.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=default NODE_ENV=production webpack",
    "watch": "cross-env BABEL_ENV=default webpack --watch",
    "lint:js": "eslint assets/js --ext=js",
    "git:update-hooks": "rm -r .git/hooks && node ./node_modules/husky/husky.js install"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@wordpress/babel-preset-default": "4.3.0",
    "@wordpress/browserslist-config": "2.5.0",
    "autoprefixer": "9.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.2.0",
    "eslint": "6.0.1",
    "grunt": "1.0.4",
    "grunt-checktextdomain": "1.0.1",
    "grunt-contrib-compress": "1.5.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-cssmin": "3.0.0",
    "grunt-contrib-jshint": "2.1.0",
    "grunt-contrib-uglify": "4.0.1",
    "grunt-contrib-watch": "1.1.0",
    "grunt-phpcs": "0.4.0",
    "grunt-postcss": "0.9.0",
    "grunt-rtlcss": "2.0.1",
    "grunt-sass": "3.0.2",
    "grunt-stylelint": "0.11.0",
    "grunt-wp-i18n": "1.0.3",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "node-sass": "4.12.0",
    "stylelint": "10.1.0",
    "stylelint-config-wordpress": "14.0.0",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5"
  },
  "engines": {
    "node": ">=8.9.3",
    "npm": ">=5.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.php": [
        "php -d display_errors=1 -l",
        "composer run-script phpcs-pre-commit"
      ],
      "*.scss": [
        "stylelint --syntax=scss --fix",
        "git add"
      ],
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "*.min.js",
      "*.css"
    ]
  }
}
